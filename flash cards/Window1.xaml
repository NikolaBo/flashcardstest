<?xml version="1.0" encoding="utf-8"?>
<Window
	x:Class="flash_cards.Window1" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Title="flash_cards"
	Height="300"
	Width="300">
	<Grid>
		<TextBlock
			Height="34"
			Grid.Column="0"
			Grid.Row="0"
			VerticalAlignment="Top"
			Margin="9,10,10,0"
			TextAlignment="Center"
			HorizontalAlignment="Center"
			FontSize="25"
			x:Name="title">
			<Run
				Text="Simple Flash Cards V 0.1" />
		</TextBlock>
		<Button
			Width="97"
			Height="38"
			x:Name="newSet"
			Click="NewSet"
			Content="Create Set"
			Margin="25,166,0,0"
			VerticalAlignment="Top"
			HorizontalAlignment="Left"
			Grid.Row="0"
			Grid.Column="0" />
		<Button
			Width="97"
			Height="38"
			x:Name="importSet"
			Click="ImportSet"
			Grid.Column="0"
			Grid.Row="0"
			HorizontalAlignment="Left"
			VerticalAlignment="Top"
			Margin="164,166,0,0"
			Content="Import Set" />
	</Grid>
</Window>