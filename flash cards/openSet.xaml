<?xml version="1.0" encoding="utf-8"?>
<Window
	x:Class="flash_cards.openSet" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Title="flash_cards"
	Height="300"
	Width="300">
	<Grid>
		<TextBlock
			FontSize="24"
			Grid.Column="0"
			Grid.Row="0"
			HorizontalAlignment="Left"
			VerticalAlignment="Top"
			Margin="96,19,0,0"
			x:Name="titleBlock">
			<Run
				Text="Open Set" />
		</TextBlock>
		<TextBox
			Width="120"
			x:Name="pathBox"
			Height="20"
			Grid.Column="0"
			Grid.Row="0"
			HorizontalAlignment="Left"
			VerticalAlignment="Bottom"
			Margin="85.5,0,0,124" />
		<TextBlock
			x:Name="pathTitle"
			Grid.Column="0"
			Grid.Row="0"
			HorizontalAlignment="Left"
			VerticalAlignment="Top"
			Margin="132.5,97,0,0">
			<Run
				Text="Path:" />
		</TextBlock>
		<Button
			Width="75"
			Height="23"
			x:Name="openButton"
			Content="Open"
			Grid.Column="0"
			Grid.Row="0"
			HorizontalAlignment="Left"
			VerticalAlignment="Top"
			Margin="107.5,164,0,0"
			RenderTransformOrigin="0.5,0.5"
			Click="openButton_Click" />
	</Grid>
</Window>